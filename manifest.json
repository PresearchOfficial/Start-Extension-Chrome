{
  "manifest_version": 2,

  "name": "Presearch.org Start With Us",
  "description": "Set Chrome's Homepage and New Tab page to Presearch.org",
  "version": "1.0.1",
  "homepage_url": "https://www.presearch.org/?utm_source=extcr",
  "icons": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.presearch.org/*"],
      "css": ["assets/popup-visibility.css"],
      "js": ["assets/popup-visibility.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "*://*.presearch.org/*"
  ],
  "chrome_settings_overrides": {
    "homepage": "https://www.presearch.org/?utm_source=extcr",
    "startup_pages": ["https://www.presearch.org/?utm_source=extcr"]
  },
  "chrome_url_overrides" : {
     "newtab": "newtab.html"
  }
}
